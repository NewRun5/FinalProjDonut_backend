type Curriculum {
    id: ID!
    userId: Int!
    title: String!
    description: String!
    imagePath: String
    progress: Float!
    createDate: LocalDate!
    completeDate: LocalDate
    deleteDate: LocalDate
    chapterList: [Chapter]
    isRagUsed: Boolean
}
input insertCurriculum{
    title: String!
    description: String!
    chapterList: [insertCurriculumChapterList]!
    isRagUsed: Boolean!
}
input insertCurriculumChapterList {
    title: String!
    description: String!
    goal: String!
}
extend type Mutation {
    saveCurriculum(input: insertCurriculum): Int
}
extend type Query {
    getCurriculumById(id: Int!): Curriculum
}